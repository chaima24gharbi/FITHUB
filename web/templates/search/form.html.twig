        <div style="margin-left: 520px;">
            <button class="btn btn-primary" id="ajax-search" style="background-color:black;color:#FAC031; border:none; border-radius:30px;">{{button_label|default('Search')}}</button>
        </div>
        <div style="margin-left: 200px; margin-top: -35px;">
            <a href="{{path('app_consultation_index')}}" class="btn btn-success"style="background-color:black;color:#FAC031; border:none; border-radius:30px;" >{{button_label|default('View All')}}</a>
